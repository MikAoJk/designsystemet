{
  "name": "@skatteetaten/designsystemet",
  "groupId": "no.skatteetaten.aurora",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "private": false,
  "author": {
    "name": "Skatteetaten"
  },
  "files": [
    "dist",
    "metadata",
    "git-properties.json"
  ],
  "engines": {
    "npm": ">=8.3.0"
  },
  "publishConfig": {
    "registry": "https://nexus.sits.no/repository/npm-internal/"
  },
  "scripts": {
    "start": "nx build ds-core-designtokens && nx serve --host=127.0.0.1",
    "start:static": "npx http-server dist/storybook/ds-playground --port 4400",
    "prepare": "node -e \"if (process.env.CI !== 'true'){process.exit(1)} \" || npx lefthook install -a",
    "prebuild": "npm-run-all lint stylelint prettier typecheck",
    "build": "nx run-many --target=build --parallel=8",
    "build:playground": "nx build ds-core-designtokens && nx build",
    "build:analyze": "STORYBOOK_WEBPACK_STATS=true nx build",
    "test": "rimraf .nyc_output/* && STORYSHOTS_MODE=static nx run-many --target=test -- --detectOpenHandles",
    "test:linux": "npx nx run-many --target=test -- --url=http://127.0.0.1:4400",
    "test:storybook": "nx test ds-playground -- --url=http://host.docker.internal:4400/",
    "test:storybook:lima": "nx test ds-playground -- --url=http://host.lima.internal:4400/",
    "test:collectAndMergeCoverage": "npx nx run-many --target=test -- --url=http://127.0.0.1:4400 --coverage && ts-node --project tsconfig.base.json libs/ds-dev-config/src/scripts/mergeCoverage.ts",
    "test:update:old": "rimraf .nyc_output/* && rimraf apps/ds-playground/__image_snapshots__/* && STORYSHOTS_MODE=static nx run-many --target=test -- -u",
    "test:update": "JEST_IMAGE_SNAPSHOT_TRACK_OBSOLETE=1 nx test ds-playground -- --url=http://host.docker.internal:4400/ -u",
    "test:update:lima": "JEST_IMAGE_SNAPSHOT_TRACK_OBSOLETE=1 nx test-storybook -- --url=http://host.lima.internal:4400/ -u",
    "test:coverage:old": "rimraf .nyc_output/* && CI=true STORYSHOTS_MODE=ci nx run-many --target=test-old",
    "test:coverage:new": "FORCE_COLOR=false CI=true npm-run-all -r -p 'start:static -- --silent' test:collectAndMergeCoverage",
    "test:coverage": "npm-run-all test:coverage:old test:coverage:new",
    "generate:coverage": "nyc report --report-dir=coverage/apps/stories --reporter=html --reporter=lcov",
    "typecheck": "nx run-many --target=tsc --parallel=8",
    "clean": "nx clear-cache",
    "lint": "nx run-many --target=lint --parallel=8",
    "lint:fix": "nx run-many --target=lint -- --fix",
    "lint:affected": "nx affected --target=lint",
    "stylelint": "nx run-many --parallel=8 --target=stylelint",
    "stylelint:fix": "nx run-many --target=stylelint --fix",
    "prettier": "nx format:check",
    "prettier:fix": "nx format",
    "sonarqube": "sonar-scanner",
    "generate:iconIndexFiles": "node ./libs/ds-icons/src/createIconIndexFiles.js",
    "generate:designtokens": "node ./libs/ds-core-designtokens/scripts/CSStoJS.js && nx format ds-core-designtokens",
    "docker:start": "docker run -d --rm --name snapshotscontainer -p 3000:3000 -e 'CONNECTION_TIMEOUT=600000' -v $(pwd)/dist/storybook/ds-playground:/opt/ds-playground -e ALLOW_FILE_PROTOCOL=true browserless/chrome:1.56-puppeteer-16.2.0",
    "docker:start:linux": "docker run -d --rm --name snapshotscontainer -e 'CONNECTION_TIMEOUT=600000' -v $(pwd)/dist/storybook/ds-playground:/opt/ds-playground -e ALLOW_FILE_PROTOCOL=true --net=host browserless/chrome:1.56-puppeteer-16.2.0",
    "docker:stop": "docker stop snapshotscontainer",
    "downloadTranslations": "TEKSTLISTEKATALOG=designsystemet TEKSTLISTER='Shared,ds-alerts,ds-buttons,ds-forms,ds-pages,ds-tables' TRANSLATIONS_FOLDER='./libs/ds-core-utils/src/translations' ts-node  --project tsconfig.base.json  ./libs/ds-dev-config/src/scripts/downloadTranslations.ts"
  },
  "dependencies": {
    "@skatteetaten/ds-core-designtokens": "file:dist/libs/ds-core-designtokens",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-storyshots": "6.5.12",
    "@storybook/addon-storyshots-puppeteer": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/core-server": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/react": "6.5.12",
    "core-js": "3.24.1",
